{
    "manifest_version": 3,
    "name": "Learning-By-GPT",
    "version": "1.0.0",
    "content_scripts": [
        {
          "matches": ["https://chat.openai.com/chat"],
          "js": ["src/content-script"]
        }
    ],
    "background": {
      "service_worker": "src/background.ts",
      "type": "module"
  },
    "permissions": ["webRequest", "https://*.openai.com/"],
    "action": { "default_popup": "index.html" }
}